        },
        {
          "id": "ql_client_secret",
          "name": "青龙 Client Secret",
          "val": "",
          "type": "text",
          "placeholder": "请输入 Client Secret",
          "desc": "在青龙面板系统设置-应用设置中创建应用后获取"
        },
        {
          "id": "wskeyList",
          "name": "WSKEY 列表",
